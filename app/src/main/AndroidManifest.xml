<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rishi.dash3">

    <application
        android:allowBackup="true"
        android:name=".DashEntry"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/DarkTheme">
        <activity
            android:name=".Activties.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/DarkTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activties.CourseInfo"/>
        <activity android:name=".Activties.AllCourses"/>
        <activity android:name=".Activties.CalendarActivity"/>
    </application>

</manifest>